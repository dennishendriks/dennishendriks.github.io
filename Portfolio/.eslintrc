{
  "extends": "airbnb-base",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "indent": [2, 4],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
  },
  "globals": {
    "window": true,
    "document": true,
  }
}
